#Valid Login
object[browserType,browser]
object[username]=//input[@placeholder='Username'
object[Password]=//input[@placeholder='Password'
object[Login]=//span[contains(text(),'Login')
object[LOGOUT]=//a[normalize-space()='LOGOUT']
#compare[Expected,Actual]

#invalid login
object[Username]=//input[@placeholder='Username'
object[Password]=//input[@placeholder='Password'
object[Login]=//span[contains(text(),'Login')
object[errormessage]=//div[@id='error']

